	</parameter>
	<queryString>
		<![CDATA[SELECT
notacredito.nc_codigo AS Nro,
notacredito.nc_fecha AS fecha,
notacredito.nc_factura AS factura,
clientes.cli_razonsocial AS razon,
clientes.cli_ruc AS RUC,
clientes.cli_descuento AS desCli,
clientes.cli_telefono AS telef,
detallenotacredito.dn_cantidad AS cant,
articulo.art_descripcion AS articulo,
detallenotacredito.dn_precarticulo AS precio,
detallenotacredito.dn_total AS moonto,
notacredito.nc_descuendo AS descu,
notacredito.nc_total AS total
FROM
notacredito
INNER JOIN clientes ON notacredito.nc_cliente = clientes.cli_codigo
INNER JOIN detallenotacredito ON detallenotacredito.nc_codigo = notacredito.nc_codigo
INNER JOIN articulo ON detallenotacredito.dn_articulo = articulo.art_codigo
WHERE notacredito.nc_codigo = $P{ls_codigo}]]>
	</queryString>
